{
  "name": "responsive-chrome-extension",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "append-query": "^2.0.1",
    "array-move": "^2.1.0",
    "chrome-extension-async": "^3.3.2",
    "classnames": "^2.2.6",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "query-string": "^6.8.2",
    "react": "^16.8.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-scripts": "3.0.1",
    "react-sortable-hoc": "^1.9.1",
    "redux": "^4.0.4",
    "redux-form": "^8.2.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.5",
    "scroll-into-view": "^1.9.7",
    "url-parse": "^1.4.7",
    "uuid": "^3.3.2",
    "validator": "^11.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build && yarn run build:bg",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:bg": "webpack --mode production ./src/background.js --output ./build/background.js && cp ./public/manifest.json ./build/manifest.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "webpack-cli": "^3.3.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
